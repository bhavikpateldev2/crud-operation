{
  "env": {
    "browser": true,
    "es2021": true,
  },
  "extends": [  "eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "ignorePatterns": ["*.spec.js"],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "off",
    "react/prop-types": 0,
    "linebreak-style": ["error", "unix"]
  }
}
